msgid "plugins.generic.sushi.name"
msgstr "SUSHI-Lite"

msgid "plugins.generic.sushi.description"
msgstr "Provide journal usage statistics through a REST API that implements the SUSHI-Lite protocol. Journal statistics follow the COUNTER guidelines."

msgid "plugins.generic.sushi.api.invalidCustomerId"
msgstr "You must supply a valid customer_id to access this report."

msgid "plugins.generic.sushi.api.invalidDates"
msgstr "You must supply a valid begin_date and end_date to access this report."

msgid "plugins.generic.sushi.api.invalidCount"
msgstr "You can only request a count between 1 and 100."

msgid "plugins.generic.sushi.api.invalidPositionToken"
msgstr "You must supply a position_token that is 0 or greater."
